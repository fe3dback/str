    "homepage": "https://github.com/fe3dback/str",
    "license": "MIT",
    "authors": [
        {
            "name": "fe3dback",
            "email": "fe3dback@yandex.ru"
        },
        {
            "name": "irhen",
            "email": "personalwonderland@gmail.com"
        },
        {
            "name": "Daniel St. Jules",
